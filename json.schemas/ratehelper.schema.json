{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "ratehelper.schema.json",
    "description": "Rate helper schema specification",
    "type": "object",
    "properties": {
        "helperType": {
            "description": "Rate Helper Type",
            "type": "string",
            "enum": [
                "Deposit",
                "Swap",
                "OIS",
                "Xccy",
                "Bond",
                "FxSwap",
                "XccyBasis",
                "TenorBasis"
            ]
        },
        "helperConfig": {
            "type": "object"
        },
        "marketConfig": {
            "type": "object"
        }
    },
    "required": [
        "helperType",
        "helperConfig",
        "marketConfig"
    ]
}